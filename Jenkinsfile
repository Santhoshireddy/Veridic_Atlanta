#!/usr/bin/env groovy

pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                echo " Hello World"
            }
        }
        stage('Test'){
            steps {
                echo " This is a Test Stage"
            }
        }
        stage('Deploy') {
            steps {
                echo " This is a Deploy stage"
            }
        }
    }
}